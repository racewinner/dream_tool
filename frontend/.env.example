# Frontend Environment Configuration
# Copy this file to .env and update values for your environment

# API Base URL - Backend server endpoint
VITE_API_BASE_URL=http://localhost:3001

# Production API URL (uncomment for production)
# VITE_API_BASE_URL=https://your-production-api.com

# Development settings
VITE_NODE_ENV=development

# Enable/disable debug logging
VITE_DEBUG_LOGGING=true

# Authentication settings
VITE_TOKEN_STORAGE_KEY=dream_tool_token
VITE_TOKEN_EXPIRY_HOURS=24

# Import settings
VITE_MAX_FILE_SIZE_MB=50
VITE_SUPPORTED_FILE_TYPES=csv,json

# Rate limiting (requests per minute)
VITE_RATE_LIMIT_RPM=60
